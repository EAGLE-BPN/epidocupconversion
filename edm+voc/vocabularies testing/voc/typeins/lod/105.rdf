        <skos:prefLabel xml:lang="la">tit. in artis operib. inscr.</skos:prefLabel>
        <skos:altLabel xml:lang="de">(Künstler-)Signatur</skos:altLabel>
        <skos:altLabel xml:lang="en">caption or signature</skos:altLabel>
