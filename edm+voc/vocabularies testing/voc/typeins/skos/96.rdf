        <skos:prefLabel xml:lang="la">term. non sep.</skos:prefLabel>
        <skos:altLabel xml:lang="fr">borne</skos:altLabel>
        <skos:altLabel xml:lang="en">Boundary inscription</skos:altLabel>
        <skos:altLabel xml:lang="en">Border delimitation</skos:altLabel>
        <skos:altLabel xml:lang="en">boundary marker</skos:altLabel>
        <skos:altLabel xml:lang="en">Boundary stone</skos:altLabel>
        <skos:altLabel xml:lang="en">boundary text</skos:altLabel>
        <skos:altLabel xml:lang="en">boundary</skos:altLabel>
        <skos:altLabel xml:lang="la">cippus</skos:altLabel>
