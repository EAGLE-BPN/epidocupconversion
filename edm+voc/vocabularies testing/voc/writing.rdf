            <skos:Concept rdf:about="http://www.eagle-network.eu/voc/writing/lod/34">
                <skos:prefLabel xml:lang="la">Crustis scriptus</skos:prefLabel>
            </skos:Concept>
        </skos:exactMatch>
        <dct:created>2013-08-26 11:31:29</dct:created>
    </skos:Concept>
